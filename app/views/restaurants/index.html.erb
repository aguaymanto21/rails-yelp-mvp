<h1>  Restaurants </h1>

<div id="restaurants">
  <% @restaurants.each do |restaurant| %>
    <li>
      <%= restaurant.name %><br>
      Address: <%= restaurant.address %><br>
      Category: <%= restaurant.category %><br>
      Rating: <%= restaurant.rating %>
    </li>
  <% end %>

  <%= link_to "Add new review" , new_restaurant_path %>
</div>
